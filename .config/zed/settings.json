{
  "theme": "Tokyo Night",
  "experimental.theme_overrides": {
    "background.appearance": "transparent",
    "tab.active_background": "#363b54",
    "text.muted": "#a9b1d6",
    "scrollbar.thumb.border": "#101014",
    // "element.active": "#363b54",
    "element.hover": "#363b54", // or 1a1b26
    "element.selected": "#363b54", // or 1a1b26
    // "ghost_element.active": "#363b54",
    "ghost_element.hover": "#363b54", // or 1a1b26
    "ghost_element.selected": "#363b54" // or 1a1b26
  },
  "icon_theme": "Colored Zed Icons Theme Dark",
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "edit_predictions": {
    "disabled_globs": [],
    "mode": "eager",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": true
  },
  "buffer_font_size": 14,
  "ui_font_size": 16,
  "agent_font_size": 18,
  "inline_code_actions": false,
  "vim_mode": true,
  "show_edit_predictions": true,
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
    "selections_menu": true
  },
  "use_system_window_tabs": true,
  "title_bar": {
    "show_user_picture": false
  },
  "scrollbar": {
    "show": "auto",
    "git_diff": false,
    "diagnostics": "all"
  },
  "gutter": {
    "runnables": false
  },
  "scroll_beyond_last_line": "off",
  "search": {
    "button": false
  },
  // "inlay_hints": {
  //   "enabled": true,
  //   "show_type_hints": true,
  //   "show_parameter_hints": true,
  //   "show_other_hints": true
  // },
  "project_panel": {
    "auto_fold_dirs": false,
    "button": false
  },
  "outline_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "git_panel": {
    "button": false
  },
  "notification_panel": {
    "button": false
  },
  "agent": {
    "dock": "right",
    "play_sound_when_agent_done": true,
    "model_parameters": [],
    "profiles": {
      "write": {
        "name": "Write",
        "tools": {
          "create_directory": true,
          "open": true,
          "batch_tool": true,
          "code_actions": true,
          "code_symbols": true,
          "contents": true,
          "copy_path": true,
          "create_file": true,
          "delete_path": true,
          "diagnostics": true,
          "edit_file": true,
          "fetch": true,
          "list_directory": true,
          "move_path": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "grep": true,
          "rename": true,
          "symbol_info": true,
          "terminal": true,
          "thinking": true,
          "web_search": true
        },
        "enable_all_context_servers": true,
        "context_servers": {}
      },
      "ask": {
        "name": "Ask",
        "tools": {
          "copy_path": true,
          "terminal": false,
          "symbol_info": true,
          "open": true,
          "contents": true,
          "diagnostics": true,
          "fetch": true,
          "list_directory": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "grep": true,
          "thinking": true,
          "web_search": true
        },
        "enable_all_context_servers": true,
        "context_servers": {
          "supabase": {
            "tools": {
              "restore_project": false,
              "reset_branch": false,
              "rebase_branch": false,
              "pause_project": false,
              "merge_branch": false,
              "execute_sql": false,
              "deploy_edge_function": false,
              "delete_branch": false,
              "create_project": false,
              "create_branch": false,
              "confirm_cost": false,
              "apply_migration": false
            }
          },
          "postgres-context-server": {
            "tools": {
              "query": true,
              "pg-schema": true
            }
          },
          "mcp-server-context7": {
            "tools": {
              "resolve-library-id": true,
              "get-library-docs": true
            }
          }
        }
      }
    },
    "always_allow_tool_actions": true,
    "default_profile": "ask",
    "default_width": 640,
    "default_model": {
      "provider": "anthropic",
      "model": "claude-sonnet-4-latest"
    }
  },
  "tab_bar": {
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false
  },
  "tabs": {
    "git_status": true
  },
  "soft_wrap": "editor_width",
  "tab_size": 2,
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "journal": {
    "hour_format": "hour24"
  },
  "terminal": {
    // "dock": "right",
    "blinking": "on",
    // "button": false,
    "line_height": "standard",
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "code_actions_on_format": {
    "source.fixAll.eslint": true
  },
  "file_types": {
    "env": [".env.*"],
    "ini": [".npmrc"]
  },
  "languages": {
    "SQL": {
      "enable_language_server": true,
      "formatter": {
        "external": {
          "command": "pg_format",
          "arguments": [
            "--comma-end",
            "--comma-break",
            "--keyword-case",
            "1",
            "--function-case",
            "1",
            "--type-case",
            "1",
            "--spaces",
            "2",
            "--format-type",
            "-"
          ]
        }
      }
    },
    "TOML": {
      "enable_language_server": true,
      "formatter": {
        "external": {
          "command": "taplo",
          "arguments": ["fmt", "-"]
        }
      }
    }
  },
  "language_models": {
    "anthropic": {
      "api_url": "https://api.anthropic.com"
    },
    "google": {
      "api_url": "https://generativelanguage.googleapis.com"
    },
    "ollama": {
      "api_url": "http://localhost:11434"
    },
    "openai": {
      "api_url": "https://api.openai.com/v1"
    },
    "lmstudio": {
      "api_url": "http://localhost:1234/api/v0"
    },
    "deepseek": {
      "api_url": "https://api.deepseek.com"
    }
  },
  "preview": {
    "theme": null
  },
  "command_aliases": {
    "W": "w"
  },
  "context_servers": {
    "mcpServers": {
      "source": "extension",
      "settings": {}
    }
  }
}
